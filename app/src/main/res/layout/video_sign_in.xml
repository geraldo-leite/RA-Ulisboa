<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    app:barrierAllowsGoneWidgets="false"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="25dp">

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="start service"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="282dp" />

    <Button
        android:id="@+id/stop"
        android:layout_width="117dp"
        android:layout_height="3dp"
        android:text="stop service"
        tools:layout_editor_absoluteX="-12dp"
        tools:layout_editor_absoluteY="-18dp" />

    <TextView
        android:id="@+id/latitude"
        android:layout_width="0dp"
        android:layout_height="37dp"
        tools:layout_editor_absoluteX="411dp" />

    <TextView
        android:id="@+id/longitude"
        android:layout_width="0dp"
        android:layout_height="25dp" />


    <Button
        android:id="@+id/getone"
        style="?attr/radioButtonStyle"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginStart="256dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="45dp"
        android:background="@drawable/messenger_button_send_round_shadow"
        android:backgroundTintMode="src_over"
        android:text="Check"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/cardview_light_background"
        android:textColorHighlight="@color/common_google_signin_btn_text_light_default"
        android:textColorHint="#32FFFFFF"
        android:textColorLink="@android:color/transparent"
        android:textCursorDrawable="@drawable/messenger_button_send_round_shadow"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />


    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="11dp" />

    <android.support.constraint.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="11dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="231dp"
        android:layout_height="154dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="278dp"
        app:layout_constraintBottom_toTopOf="@+id/getone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/transparent" />

</android.support.constraint.ConstraintLayout>